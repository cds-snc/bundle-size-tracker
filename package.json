{
  "name": "bundle-size-tracker",
  "private": true,
  "version": "1.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/index.js --watch src/lib --watch src/index.js",
    "test": "jest",
    "lint": "eslint src/*.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@octokit/rest": "^16.2.0",
    "aws-sdk": "^2.373.0",
    "dotenv-safe": "^6.1.0",
    "esm": "^3.0.84",
    "nodemon": "^1.18.8",
    "pretty-bytes": "^5.1.0",
    "serverless-google-cloudfunctions": "^2.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "nock": "^10.0.4",
    "prettier": "^1.15.3"
  }
}
